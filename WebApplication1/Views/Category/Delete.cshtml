@model Category

<h1>Delete Category</h1>
<form method="post">

    <div class="form-outline mb-4">
        <label asp-for="Name"></label>
        <input disabled asp-for="Name" type="text" class="form-control" />

    </div>

    <div class="form-outline mb-4">
        <label asp-for="DisplayOrder"></label>
        <input disabled asp-for="DisplayOrder" class="form-control" />

    </div>
    <button asp-controller="Category" asp-action="Delete" class="btn btn-danger">Delete</button>
    <button class="btn btn-primary" asp-action="Index" asp-controller="Category">Back To List</button>
</form>